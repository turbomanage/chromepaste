{
  "name": "Paste On Screen",
  "version": "1.01",
  "manifest_version": 2,
  "description": "Show contents of the clipboard in a popup with large font.",
  "permissions": [
    "clipboardRead",
    "storage"
  ],
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_title": "Paste On Screen",
    "default_popup": "popup.html"
  },
  "icons": {
    "32": "clip32.png",
    "64": "clip64.png",
    "128": "clip128.png"
  }
}
